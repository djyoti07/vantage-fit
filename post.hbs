{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}


{{!-- Everything inside the #post tags pulls data from the post --}}
{{#post}}


<div class="elementor">
    <div class="elementor-inner">
        <div class="elementor-section-wrap">
        	<section class="breadcrumb_area breadcrumb_area_two vc-btn-gradient-color vc-btn-gradient-general"> 
	        	<img class="breadcrumb_shap hidden-xs " src="https://res.cloudinary.com/vantagecircle/image/upload/v1580904957/VantageFit/website/lines.png" alt="lines">
	            <img class="breadcrumb_pulse hidden-xs" src="https://res.cloudinary.com/vantagecircle/image/upload/v1580904957/VantageFit/website/heart-lines.png" alt="pulse">
	        	<div class="container">
	        		<div class="breadcrumb_content text-center main_title  wow fadeInUp animated " data-wow-delay=".1s" style="visibility: visible;-webkit-animation-delay: .1s; -moz-animation-delay: .1s; animation-delay: .1s;">
	        			<h1 class="post-title"> {{title}}</h1>
	        		</div>
	        	</div> 
	        </section>
        	<div class="main-content">
			    <section>
			        <div class="container custom-container">
			            <div class="row post-content details-post">
			            	
            			<div class="col-lg-9 col-md-12 col-sm-12 col-xs-12">{{content}}</div>
            			<div class="col-lg-3 col-md-12 col-sm-12 col-xs-12 ">
            				<div class="downloadsection  wow fadeInRight text-center" data-wow-delay="0.4s" style="visibility: visible;-webkit-animation-delay: 0.4s; -moz-animation-delay: 0.4s; animation-delay: 0.4s;">
								<div>
									<h3 style="color:#ffffff">Free eBook</h3>
									A complete guide to wellness program
								</div>
								<img src="https://res.cloudinary.com/vantagecircle/image/upload/v1580904957/VantageFit/website/_guide-to-corporate-wellness.png" alt="wellnessprogram" >
								
								<div class="transform">
						            <a class="popup-youtube banner_btn white_button button-style" href="https://www.vantagecircle.com/en/resources/free-ebook-corporate-wellness-program/" target="_blank">
						               <span style="color: #ffffff;">&nbsp;&nbsp;Free Download </span>
						            </a>
						          </div>
							</div>
            				<div class="sticky">
	            				{{#get "posts" include="tags,authors" limit="6"  order="published_at desc"}}
	            				<div class="rightsidebar  wow fadeInRight " data-wow-delay="0.4s" style="visibility: visible;-webkit-animation-delay: 0.4s; -moz-animation-delay: 0.4s; animation-delay: 0.4s;">
	            				<div class="post-title">Latest Post
	            				</div>
	            				
								    {{#foreach posts}}
								    <div class="row d-flex align_items_center ind-row">
								    <div class="col-lg-4 col-md-4 col-xs-12 col-sm-12 pclr"><a href="{{url}}"><img class="img-ind img-responsive" src="{{img_url feature_image size="m"}}" alt="{{title}}"/></a></div>
	        							
	        						<div class="col-lg-8  col-md-8 col-xs-12 col-sm-12" style="line-height:1.5"><a href="{{url}}" class="title-post-ind">{{title}}</a></div>
	        						</div>
								    {{/foreach}}
								
								</div>
		            			
								{{/get}}


							</div>
						</div>
							
	            		</div>
	            	</div>
	            </section>
	        </div>
        </div>
    </div>
</div>

{{/post}}
