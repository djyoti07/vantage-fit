{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}


{{!-- Everything inside the #post tags pulls data from the post --}}
{{#post}}

<div class="elementor">
    <div class="elementor-inner">
        <div class="elementor-section-wrap">
        <section class="breadcrumb_area breadcrumb_area_two vc-btn-gradient-color vc-btn-gradient-general"> 
        	<img class="breadcrumb_shap hidden-xs " src="https://res.cloudinary.com/vantagecircle/image/upload/w_500/v1580904957/VantageFit/website/lines.png" alt="lines">
            <div class="marketinglanding {{post_class}}">
                <img class="breadcrumb_pulse hidden-xs" src="https://res.cloudinary.com/vantagecircle/image/upload/w_300/v1580904957/VantageFit/website/heart-lines.png" alt="pulse">
            </div>
            <div class="featuredpage {{post_class}}">
                <img class="breadcrumb_pulse hidden-xs campaign hidden-sm hidden-sml" src="https://res.cloudinary.com/vantagecircle/image/upload/w_500/v1586230738/VantageFit/website/topheader.png" alt="pulse">
            </div>
        	<div class="container">
        		<div class="breadcrumb_content text-center main_title">
        			<h1> {{title}}</h1>
                    {{#if custom_excerpt}}
                    <p style="color:#ffffff">{{custom_excerpt}}</p>
                    {{/if}}
        		</div>
        	</div> 
        </section>
		    {{content}}
        </div>
    </div>
</div>

{{/post}}


